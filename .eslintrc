{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    }
  },
  "rules": {
    "no-use-before-define": "off",
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
      
    ],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
   ]
  }
}
